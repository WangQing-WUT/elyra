{
	"type": "object",
	"properties": {
		"name": {
			"title": "Name",
			"description": "A custom Name for the node.",
			"type": "string"
		},
		"label": {
			"title": "Label",
			"description": "A custom label for the node.",
			"type": "string"
		},
		"component_parameters": {
			"type": "object",
			"properties": {
				{% if  component.name == "Dataset Event" %}
				"event_filter": {
					"type": "array",
					"title": "Dataset Event Filter",
					"description": "Dataset event filter.",
					"items": {
						"type": "object",
						"properties": {
							"id": {
								"type": "string",
								"title": "Id"
							},
							"name": {
								"type": "string",
								"title": "Name",
								"enum": ["name", "operate", "samples", "create_at", "publish_status", "subscribe_status"]
							}
						},
						"allOf": [
							{
								"if": {
        							"properties": {
          								"name": {
           					 				"const": "name"
     						  			}
       								}
      							},
								"then": {
        							"properties": {
          								"operate": {
            								"type": "string",
											"title": "Operate",
											"enum": ["=="]
         								},
										"value": {
											"type": "string",
											"title": "Value"
										}
       								},
        							"required": ["value"]
								}
							},
							{
								"if": {
        							"properties": {
          								"name": {
           					 				"const": "operate"
     						  			}
       								}
      							},
								"then": {
        							"properties": {
          								"operate": {
            								"type": "string",
											"title": "Operate",
											"enum": ["=="]
         								},
										"value": {
											"type": "string",
											"title": "Value"
										}
       								},
        							"required": ["value"]
								}
							},
							{
								"if": {
        							"properties": {
          								"name": {
           					 				"const": "samples"
     						  			}
       								}
      							},
								"then": {
        							"properties": {
          								"operate": {
            								"type": "string",
											"title": "Operate",
											"enum": [">", "<", "==", ">=", "<=", "!=" ]
         								},
										"value": {
											"type": "integer",
											"title": "Value"
										}
       								},
        							"required": ["value"]
								}
							},
							{
								"if": {
        							"properties": {
          								"name": {
           					 				"const": "create_at"
     						  			}
       								}
      							},
								"then": {
        							"properties": {
          								"operate": {
            								"type": "string",
											"title": "Operate",
											"enum": ["before", "after"]
         								},
										"value": {
											"type": "string",
											"title": "Value",
											"format": "datetime"
										}
       								},
        							"required": ["value"]
								}
							},
							{
								"if": {
        							"properties": {
          								"name": {
           					 				"enum": ["publish_status", "subscribe_status"]
     						  			}
       								}
      							},
								"then": {
        							"properties": {
          								"operate": {
            								"type": "string",
											"title": "Operate",
											"enum": ["=="]
         								},
										"value": {
											"type": "string",
											"title": "Value",
											"enum": ["creat", "finish"]
										}
       								},
        							"required": ["value"]
								}
							}
						]
					},
					"uihints":{
						"items": {
							"id": { "ui:readonly": "true" }
						}
					}
				},
				{% endif %}
			    {% if  component.name == "Model Event" %}
				"event_filter": {
					"type": "array",
					"title": "Model Event Filter",
					"description": "Model event filter.",
					"items": {
						"type": "object",
						"properties": {
							"id": {
								"type": "string",
								"title": "Id"
							},
							"name": {
								"type": "string",
								"title": "Name",
								"enum": ["name", "operate", "version", "create_at", "update_at"]
							}
						},
						"allOf": [
							{
								"if": {
        							"properties": {
          								"name": {
           					 				"const": "name"
     						  			}
       								}
      							},
								"then": {
        							"properties": {
          								"operate": {
            								"type": "string",
											"title": "Operate",
											"enum": ["=="]
         								},
										"value": {
											"type": "string",
											"title": "Value"
										}
       								},
        							"required": ["value"]
								}
							},
							{
								"if": {
        							"properties": {
          								"name": {
           					 				"const": "operate"
     						  			}
       								}
      							},
								"then": {
        							"properties": {
          								"operate": {
            								"type": "string",
											"title": "Operate",
											"enum": ["=="]
         								},
										"value": {
											"type": "string",
											"title": "Value"
										}
       								},
        							"required": ["value"]
								}
							},
							{
								"if": {
        							"properties": {
          								"name": {
           					 				"const": "version"
     						  			}
       								}
      							},
								"then": {
        							"properties": {
          								"operate": {
            								"type": "string",
											"title": "Operate",
											"enum": ["=="]
         								},
										"value": {
											"type": "string",
											"title": "Value"
										}
       								},
        							"required": ["value"]
								}
							},
							{
								"if": {
        							"properties": {
          								"name": {
           					 				"enum": ["create_at", "update_at"]
     						  			}
       								}
      							},
								"then": {
        							"properties": {
          								"operate": {
            								"type": "string",
											"title": "Operate",
											"enum": ["before", "after"]
         								},
										"value": {
											"type": "string",
											"title": "Value",
											"format": "datetime"
										}
       								},
        							"required": ["value"]
								}
							}
						]
					},
					"uihints":{
						"items": {
							"id": { "ui:readonly": "true" }
						}
					}
				},
				{% endif %}
				{% if  component.name == "MinIO Event" %}
				"endpoint": {
					"type": "string",
					"title": "Minio Endpoint",
					"description": "Minio endpoint.",
					"format": "uri",
					"uihints": {
						"ui:placeholder": "minio-service.argo-events:9000"
					}
				},
				"accesskey": {
					"type": "string",
					"title": "AccessKey",
					"description": "Minio accesskey.",
					"uihints": {
						"ui:field": "password"
					}
				},
				"secretkey": {
					"type": "string",
					"title": "SecretKey",
					"description": "Minio secretkey.",
					"uihints": {
						"ui:field": "password"
					}
				},
				"event_filter": {
					"type": "array",
					"title": "Minio Event Filter",
					"description": "Minio event filter.",
					"items": {
						"type": "object",
						"properties": {
							"id": {
								"type": "string",
								"title": "Id"
							},
							"name": {
								"type": "string",
								"title": "Name",
								"enum": ["operate", "object", "bucket"]
							}
						},
						"allOf": [
							{
								"if": {
        							"properties": {
          								"name": {
           					 				"const": "operate"
     						  			}
       								}
      							},
								"then": {
        							"properties": {
          								"operate": {
            								"type": "string",
											"title": "Operate",
											"enum": ["==", "!="]
         								},
										"value": {
											"type": "string",
											"title": "Value"
										}
       								},
        							"required": ["value"]
								}
							},
							{
								"if": {
        							"properties": {
          								"name": {
           					 				"const": "object"
     						  			}
       								}
      							},
								"then": {
        							"properties": {
          								"operate": {
            								"type": "string",
											"title": "Operate",
											"enum": ["prefix", "suffix"]
         								},
										"value": {
											"type": "string",
											"title": "Value"
										}
       								},
        							"required": ["value"]
								}
							}
						]
					},
					"uihints":{
						"items": {
							"id": { "ui:readonly": "true" }
						}
					}
				},
				{% endif %}
				{% if  component.name == "Pipeline Event" %}
				"event_filter": {
					"type": "array",
					"title": "Pipeline Event Filter",
					"description": "Pipeline event filter.",
					"items": {
						"type": "object",
						"properties": {
							"id": {
								"type": "string",
								"title": "Id"
							},
							"name": {
								"type": "string",
								"title": "Name",
								"enum": ["name", "operate", "label_selector", "create_at"]
							}
						},
						"allOf": [
							{
								"if": {
        							"properties": {
          								"name": {
           					 				"const": "name"
     						  			}
       								}
      							},
								"then": {
        							"properties": {
          								"operate": {
            								"type": "string",
											"title": "Operate",
											"enum": ["==", "prefix"]
         								},
										"value": {
											"type": "string",
											"title": "Value"
										}
       								},
        							"required": ["value"]
								}
							},
							{
								"if": {
        							"properties": {
          								"name": {
           					 				"const": "operate"
     						  			}
       								}
      							},
								"then": {
        							"properties": {
          								"operate": {
            								"type": "string",
											"title": "Operate",
											"enum": ["=="]
         								},
										"value": {
											"type": "string",
											"title": "Value"
										}
       								},
        							"required": ["value"]
								}
							},
							{
								"if": {
        							"properties": {
          								"name": {
           					 				"const": "label_selector"
     						  			}
       								}
      							},
								"then": {
        							"properties": {
          								"operate": {
            								"type": "string",
											"title": "Operate",
											"enum": ["=="]
         								},
										"value": {
											"type": "string",
											"title": "Value"
										}
       								},
        							"required": ["value"]
								}
							},
							{
								"if": {
        							"properties": {
          								"name": {
           					 				"const": "create_at"
     						  			}
       								}
      							},
								"then": {
        							"properties": {
          								"operate": {
            								"type": "string",
											"title": "Operate",
											"enum": ["before"]
         								},
										"value": {
											"type": "string",
											"title": "Value",
											"format": "datetime"
										}
       								},
        							"required": ["value"]
								}
							}
						]
					},
					"uihints":{
						"items": {
							"id": { "ui:readonly": "true" }
						}
					}
				},
				{% endif %}
				{% if  component.name == "Calendar Event" %}
				"interval": {
					"type": "integer",
					"title": "Interval",
					"description": "Calendar interval.",
					"minimum": 0
				},
				"schedule": {
					"type": "string",
					"title": "Schedule",
					"description": "Calendar schedule."
				}
				{% endif %}
				{% if  component.name != "Calendar Event" %}
				"expression": {
					"type": "string",
					"title": "Expression",
					"description": "{{component.name}} expression."
				}
				{% endif %}
			},
			"required": []
		}
	},
	"required": ["component_parameters"]
}